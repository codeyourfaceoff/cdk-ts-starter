{
  "name": "cdk-ts-starter",
  "version": "0.1.0",
  "bin": {
    "run": "bin/run.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "yamlOutput": "node ./scripts/parse-cf-json-to-yaml.js",
    "postdia": "pnpm cdk synth && pnpm yamlOutput",
    "dia": "pnpm cdk synth --app \"node -r tsm ./bin/diagram.ts\"",
    "dia:watch": "nodemon --watch bin --watch lib -e js,ts --exec \"pnpm dia\""
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.34.2",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.3",
    "archiver": "^5.3.1",
    "aws-cdk": "^2.34.2",
    "aws-sdk": "^2.1185.0",
    "cdk-assets": "^2.34.2",
    "cdk-dia": "^0.7.0",
    "esbuild": "^0.14.51",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "promptly": "^3.2.0",
    "proxy-agent": "^5.0.0",
    "ts-jest": "^28.0.7",
    "tsm": "^2.2.2",
    "typescript": "^4.7.4",
    "yaml": "^1.10.2"
  },
  "dependencies": {
    "@aws-prototyping-sdk/aws-arch": "^0.14.21",
    "@aws-prototyping-sdk/cdk-graph": "^0.14.21",
    "@aws-prototyping-sdk/cdk-graph-plugin-diagram": "^0.14.21",
    "aws-cdk-lib": "^2.34.2",
    "constructs": "^10.1.298",
    "projen": "^0.70.5",
    "source-map-support": "^0.5.21"
  }
}