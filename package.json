{
  "name": "cdk-ts-starter",
  "version": "0.1.0",
  "bin": {
    "run": "bin/run.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "yamlOutput": "node ./scripts/parse-cf-json-to-yaml.js",
    "predia": "npm run cdk synth && npm run yamlOutput",
    "dia": "cdk-dia ",
    "dia:watch": "nodemon --watch bin --watch lib -e js,ts --exec \"npm run dia\""
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.0.0-rc.17",
    "cdk-dia": "^0.3.0",
    "esbuild": "^0.12.15",
    "jest": "^26.4.2",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.0.0-rc.17",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
